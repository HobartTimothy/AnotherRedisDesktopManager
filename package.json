    "postcss": "^8.4.38",
    "postcss-import": "^16.1.1",
    "postcss-loader": "^8.2.0",
    "postcss-url": "^10.1.3",
    "rimraf": "^3.0.2",
    "semver": "^7.6.3",
    "shelljs": "^0.8.4",
    "shx": "^0.4.0",
    "terser-webpack-plugin": "^5.3.10",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.11.1",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.7.16",
    "webpack": "^5.103.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^5.10.0"
  },
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 9.0.0"
  },
  "browserslist": [
